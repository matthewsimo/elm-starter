{
  "name": "elm-starter",
  "version": "0.0.2",
  "description": "Starter project for working with Elm",
  "author": "Matthew Simo <matthew.a.simo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --content-base src/",
    "build": "npm run clean && webpack && npm run assets",
    "clean": "rimraf dist",
    "assets": "cp -r src/img/ dist/img/ && cp -r src/css/ dist/css/ && cp src/favicon.ico dist/favicon.ico",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "css-loader": "0.23.1",
    "elm": "0.17.0",
    "elm-hot-loader": "0.3.2",
    "elm-webpack-loader": "3.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.17.0",
    "postcss-loader": "0.9.1",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.12.0"
  },
  "dependencies": {}
}
